{
  "name": "smr-next",
  "version": "0.0.1",
  "scripts": {
    "build": "bun run translations && bun run build:static && bun run build:node",
    "build:staging": "cross-env NODE_ENV=staging bun run build:static && bun run build:node",
    "build:production": "cross-env NODE_ENV=production bun run build:static && bun run build:node",
    "build:node": "rimraf .svelte build/node && cross-env SVELTE_ADAPTER=bun vite build",
    "build:static": "rimraf .svelte build/static && vite build",
    "check": "svelte-check --ignore build,.svelte,.svelte-kit",
    "check:dev": "svelte-check --ignore build,.svelte,.svelte-kit --watch",
    "dev": "conc -c 'auto' bun:graphql-codegen:watch bun:dev:serve bun:check:dev bun:lint:dev",
    "dev:serve": "vite dev --host",
    "graphql-codegen": "graphql-codegen -r dotenv-flow/config --default-node-env=development --config codegen.yml",
    "graphql-codegen:watch": "bun graphql-codegen --watch",
    "lint": "eslint . && prettier --check .",
    "lint:dev": "chokidar -c \"eslint . --cache\" --silent --initial \"src/**/*.svelte\" \"src/**/*.ts\"",
    "prepare": "husky",
    "postinstall": "svelte-kit sync",
    "start": "svelte-kit start",
    "format": "prettier --write . && eslint . --fix",
    "translations": "bash ./download_translations.sh"
  },
  "dependencies": {
    "@felte/core": "^1.4.4",
    "@felte/reporter-svelte": "^1.2.0",
    "@felte/validator-zod": "^1.0.18",
    "@floating-ui/dom": "^1.7.4",
    "@tolgee/svelte": "^5.33.2",
    "@urql/core": "^5.2.0",
    "@urql/exchange-auth": "^2.2.1",
    "@urql/exchange-graphcache": "^7.2.4",
    "@urql/exchange-persisted": "^4.3.1",
    "@urql/introspection": "^1.2.1",
    "@urql/svelte": "^4.2.3",
    "caniuse-lite": "^1.0.30001743",
    "custom-protocol-check": "^1.4.0",
    "dompurify": "^3.2.7",
    "felte": "^1.3.0",
    "graphql": "^16.11.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^24.1.3",
    "jszip": "^3.10.1",
    "marked": "^13.0.3",
    "marked-gfm-heading-id": "^4.1.2",
    "marked-mangle": "^1.1.11",
    "prismjs": "^1.30.0",
    "semver": "^7.7.2",
    "socket.io-client": "^4.8.1",
    "thumbhash": "^0.1.1",
    "tslib": "^2.8.1",
    "wonka": "^6.3.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.8",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@graphql-codegen/urql-introspection": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@skeletonlabs/skeleton": "^2.11.0",
    "@skeletonlabs/tw-plugin": "^0.4.1",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.42.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/js-cookie": "^3.0.6",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.19.16",
    "@types/prismjs": "^1.26.5",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "cookieconsent": "^3.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.3",
    "graphql-tag": "^2.12.6",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "postcss-import-url": "^7.2.0",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "schema-dts": "^1.1.5",
    "svelte": "^5.38.10",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.3.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "urql": "^4.2.2",
    "vite": "^7.1.5",
    "vite-plugin-tailwind-purgecss": "^0.3.5"
  },
  "type": "module",
  "packageManager": "bun@1.2.22",
  "trustedDependencies": [
    "smr-next",
    "esbuild",
    "@sveltejs/kit"
  ]
}
