{
  "name": "smr-next",
  "version": "0.0.1",
  "scripts": {
    "build": "bun run translations && bun run build:static && bun run build:node",
    "build:staging": "cross-env NODE_ENV=staging bun run build:static && bun run build:node",
    "build:production": "cross-env NODE_ENV=production bun run build:static && bun run build:node",
    "build:node": "rimraf .svelte build/node && cross-env SVELTE_ADAPTER=bun vite build",
    "build:static": "rimraf .svelte build/static && vite build",
    "check": "svelte-check",
    "check:dev": "svelte-check --watch",
    "dev": "conc -c 'auto' bun:graphql-codegen:watch bun:dev:serve bun:check:dev bun:lint:dev",
    "dev:serve": "vite dev --host",
    "graphql-codegen": "graphql-codegen -r dotenv-flow/config --default-node-env=development --config codegen.yml",
    "graphql-codegen:watch": "bun graphql-codegen --watch",
    "lint": "eslint . && prettier --check .",
    "lint:dev": "chokidar -c \"eslint . --cache\" --silent --initial \"src/**/*.svelte\" \"src/**/*.ts\"",
    "prepare": "husky",
    "postinstall": "svelte-kit sync",
    "start": "bun run build/node/index.js",
    "format": "prettier --write . && eslint . --fix",
    "translations": "bash ./download_translations.sh"
  },
  "dependencies": {
    "@felte/core": "^1.4.4",
    "@felte/reporter-svelte": "^1.2.0",
    "@felte/validator-zod": "^1.0.18",
    "@floating-ui/dom": "^1.7.4",
    "@tolgee/svelte": "^6.2.7",
    "@urql/core": "^6.0.1",
    "@urql/exchange-auth": "^3.0.0",
    "@urql/exchange-graphcache": "^8.1.0",
    "@urql/exchange-persisted": "^5.0.1",
    "@urql/introspection": "^1.2.1",
    "@urql/svelte": "^5.0.0",
    "custom-protocol-check": "^1.4.0",
    "dompurify": "^3.3.0",
    "felte": "^1.3.0",
    "globals": "^16.4.0",
    "graphql": "^16.11.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^27.0.1",
    "jszip": "^3.10.1",
    "marked": "^16.4.1",
    "marked-gfm-heading-id": "^4.1.2",
    "marked-mangle": "^1.1.11",
    "prismjs": "^1.30.0",
    "semver": "^7.7.3",
    "socket.io-client": "^4.8.1",
    "thumbhash": "^0.1.1",
    "tslib": "^2.8.1",
    "wonka": "^6.3.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.2",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-document-nodes": "^5.0.2",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@graphql-codegen/urql-introspection": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@skeletonlabs/skeleton": "^4.1.4",
    "@skeletonlabs/skeleton-svelte": "^4.1.4",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "@types/js-cookie": "^3.0.6",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.9.1",
    "@types/prismjs": "^1.26.5",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001751",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cookieconsent": "^3.1.1",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.5",
    "graphql-tag": "^2.12.6",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rimraf": "^6.0.1",
    "schema-dts": "^1.1.5",
    "svelte": "^5.42.2",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.3",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "urql": "^5.0.1",
    "vite": "^7.1.12"
  },
  "type": "module",
  "packageManager": "bun@1.3.1",
  "trustedDependencies": [
    "@parcel/watcher",
    "@sveltejs/kit",
    "@tailwindcss/oxide",
    "esbuild",
    "smr-next",
    "svelte-preprocess"
  ]
}